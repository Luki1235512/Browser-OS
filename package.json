{
  "name": "browser-os",
  "version": "1.0.0",
  "description": "Desktop environment in the browser.",
  "author": "Luki1235512",
  "license": "ISC",
  "browserslist": [
    "defaults and supports es6"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "yarn build:prebuild && next build",
    "build:bundle-analyzer": "yarn build",
    "build:fs": "node scripts/fs2json.js --exclude .index --out public/.index/fs.9p.json ./public && node scripts/preloadIcons.js",
    "build:html": "node scripts/minifyHtml.js",
    "build:prebuild": "yarn build:fs && yarn build:search",
    "build:robots": "node scripts/robots.js",
    "build:search": "node scripts/searchIndex.js",
    "deploy": "yarn build:robots && next export && yarn build:html",
    "docker:build": "docker build -t cute-os .",
    "docker:run": "docker run -dp 3000:3000 --rm --name cute-os cute-os",
    "export": "next export",
    "start": "next build && next start",
    "eslint": "eslint --report-unused-disable-directives .",
    "stylelint": "stylelint --formatter=verbose **/*.ts*",
    "prettier": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "unused-exports": "ts-unused-exports tsconfig.json --excludePathsFromReport=pages",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{ts,tsx}": "stylelint --fix",
    "*.{js,ts,tsx}": "eslint --fix"
  },
  "engines": {
    "node": ">=16 <17"
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.5.1",
    "@panzoom/panzoom": "^4.5.0",
    "@wasmer/wasi": "0.12.0",
    "@wasmer/wasm-terminal": "^0.12.0",
    "@wasmer/wasm-transformer": "^0.12.0",
    "@wasmer/wasmfs": "^0.12.0",
    "ani-cursor": "^0.0.5",
    "butterchurn-presets": "^3.0.0-beta.4",
    "fflate": "^0.8.0",
    "file-type": "^17.1.2",
    "framer-motion": "10.12.16",
    "gif.js": "^0.2.0",
    "html-to-image": "^1.11.11",
    "idb": "^7.0.2",
    "ini": "^4.1.1",
    "isomorphic-git": "^1.18.3",
    "music-metadata-browser": "^2.5.10",
    "next": "^13.4.4",
    "playlist-parser": "^0.0.12",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-rnd": "^10.4.1",
    "resedit": "^1.6.0",
    "styled-components": "^5.3.11",
    "utif": "https://github.com/photopea/UTIF.js",
    "stylelint-order": "^6.0.4"
  },
  "devDependencies": {
    "7z-wasm": "^1.0.0-beta.5",
    "@next/bundle-analyzer": "^12.2.0",
    "@next/eslint-plugin-next": "^13.4.6",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/gif.js": "^0.2.2",
    "@types/ini": "^1.3.31",
    "@types/jest": "^29.5.2",
    "@types/lunr": "^2.3.4",
    "@types/node": "^20.2.5",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/react": "^18.2.7",
    "@types/styled-components": "^5.1.26",
    "@types/video.js": "^7.3.52",
    "@types/wicg-file-system-access": "^2020.9.5",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "browserfs": "https://github.com/jvilk/BrowserFS.git#a96aa2d",
    "byuu": "^0.16.0",
    "emulators": "^0.73.8",
    "emulators-ui": "^0.73.9",
    "eruda": "https://github.com/liriliri/eruda",
    "eslint": "^8.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "html-minifier-terser": "^7.0.0-beta.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.2",
    "lunr": "^2.3.9",
    "monaco-editor": "^0.39.0",
    "pdfjs-dist": "2.14.305",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^15.8.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^8.0.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "tinymce": "^6.0.3",
    "ts-unused-exports": "^8.0.0",
    "typescript": "^5.0.4",
    "webamp": "^1.5.0",
    "xlsx": "^0.18.5",
    "xterm": "^4.18.0"
  }
}
